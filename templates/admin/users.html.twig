{% extends 'base.html.twig' %}

{% block title %}Hello AdminController!{% endblock %}

{% block body %}
<div class="container my-5">
    <h1 class="fs-3">Liste des utilisateurs</h1>
    <ul class="list-group mt-3 rounded-0">
        {% for user in users %}
        <a href="{{path('app_admin_user_edit', {'id': user.id} )}}" class="list-group-item list-group-item-action">
            <span> {{user.fullName}} </span>
        </a>
        {% endfor %}
      </ul>
</div>
{% endblock %}
